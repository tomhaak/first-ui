{
  "name": "@kern-ux/native",
  "version": "2.4.0",
  "description": "Das Native Package beinhaltet alle zur Komponenten-Bibliothek geh√∂renden Styles.",
  "author": "Kern-UX",
  "license": "EUPL-1.2",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://gitlab.opencode.de/kern-ux/kern-ux-plain"
