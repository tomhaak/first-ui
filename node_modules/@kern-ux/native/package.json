  "style": "dist/kern.min.css",
  "sideEffects": false,
  "files": [
    "src",
    "dist/kern.css",
    "dist/kern.min.css",
    "dist/fonts/",
    "dist/js/kern-kopfzeile.js",
    "CHANGELOG.md"
  ],
  "keywords": [],
  "scripts": {
    "start": "npm run dev",
